    "googleapis": "^128.0.0",
    "multer": "^1.4.5-lts.1",
    "path": "^0.12.7",
    "pdf-lib": "^1.17.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11"
  },
  "keywords": [
    "google-sheets",
    "print",
    "web-app",
    "express",
    "react"
