{
  "name": "spreadsheet-to-print",
  "version": "1.0.0",
  "description": "Web application to read Google Spreadsheet data and format into printable documents",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
